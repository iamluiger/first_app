pipeline {
  agent any
  stages {
    stage('klj') {
      steps {
        pwd(tmp: true)
        echo 'ñl'
        sh 'nada'
      }
    }
    stage('lio') {
      steps {
        input(message: 'Necesitas seguir', id: 'll')
      }
    }
    stage('lui') {
      steps {
        echo 'nn'
      }
    }
  }
}